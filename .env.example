# GitHub Personal Access Token (required)
GITHUB_TOKEN=

# File extensions to analyze (comma-separated)
EXTENSIONS=.sol,.rs

# Include patterns (comma-separated, leave empty for all files)
INCLUDE=

# Exclude patterns (comma-separated)
EXCLUDE=node_modules/*,*/@openzeppelin/*,*/solmate/*,target/*,*/deps/*,*/test/*,*_test.rs,*/tests/*
